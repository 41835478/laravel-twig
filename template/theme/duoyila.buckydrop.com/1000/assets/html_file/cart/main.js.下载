        function $exceptionHandler(err, cause) {
            if (_angular.$rootScope.ERROR_STATE) {
                if (_angular.$rootScope.ERROR_PREVENTED) {
                    throw err;
                }
                return;
            }
            if (isThirdPartyFlow()) {
                if (err instanceof _squid.$Forbidden) {
                    return $forbiddenHandler(err);
                } else {
