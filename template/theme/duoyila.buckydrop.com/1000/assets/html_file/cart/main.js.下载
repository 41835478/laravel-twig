                                            });
                                            $KmliUpsell.init(upsellEligibilityData);
                                            $scope.postAuth($scope.auth);
                                        },
                                        contingencies: {
                                            DEFAULT: function handleContingency(name, contingency) {
                                                $scope.auth.password = "";
                                                $scope.errorFn(name, contingency);
                                            }
                                        }
                                    });
                                });
                            }
                            var ulEventHandlers = {
                                loginRendered: function loginRendered(data) {
