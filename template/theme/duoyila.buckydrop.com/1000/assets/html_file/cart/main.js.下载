                        var regexPattern = format === "MMYY" ? /(0\d|1[0-2])\/(\d{2})/ : /(0\d|1[0-2])\/(\d{4})/;
                        var init = function init() {
                            if (_this.expiryMonth && _this.expiryYear) {
