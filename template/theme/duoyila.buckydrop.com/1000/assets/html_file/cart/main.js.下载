                                    throw new $FallbackError({
                                        reason: "fallback_merchant_BA_nocards",
                                        product: "ux"
                                    });
                                } else {
                                    return $q.when();
                                }
                            });
                        });
                    } else {
                        return $q.when();
                    }
                }
                return function(contingency) {
