                        _logger.$logger.info(loggerName + "_success", loggerPayload);
                        if (options.success) {
                            if (options.success instanceof Function) {
                                return options.success(res.data);
                            }
                            return options.success;
                        }
                        return res;
                    }
                    if (options.failSilently) {
                        return;
                    }
                    if (res.ack === "contingency") {
