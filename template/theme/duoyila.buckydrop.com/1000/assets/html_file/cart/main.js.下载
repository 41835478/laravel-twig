                    if (!pattern.test(treatment.treatment_name)) {
                        return false;
                    }
                    result = {
                        treatment_name: treatment.treatment_name,
                        factors: treatment.factors
                    };
                    return true;
                });
                return result;
            });
        }
        function $pxpMatch(id, params, patterns, logPayload) {
            if (!patterns) {
                patterns = params;
                params = {};
            }
