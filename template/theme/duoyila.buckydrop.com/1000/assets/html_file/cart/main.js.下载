                                        pad_toggle_status: "padToggleOff"
                                    });
                                }
                                $scope.updatePlan(planId);
                            };
                            var processAllLoadedPadToggle = function processAllLoadedPadToggle() {
                                if (padToggleFeatureSupported && $scope.showPadToggle()) {
