                }
                var getKey = function getKey(p) {
                    return p && p.primaryFs && p.primaryFs.id;
                };
                var planKey = getKey(plan);
                var forKey = getKey(this.forPlan);
                if (!planKey || planKey !== forKey) {
                    return this.reset(plan);
