                            timing[key] = parseInt(window.performance.timing[key], 10) || 0;
                        });
                        var offset = timing.connectEnd - timing.navigationStart;
                        if (timing.connectEnd && window.clientStartTime) {
                            _angular2["default"].forEach(timing, function(time, name) {
