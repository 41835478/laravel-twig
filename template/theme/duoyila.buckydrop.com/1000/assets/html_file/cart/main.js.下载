                            if (ulTimeoutPromise) {
                                $timeout.cancel(ulTimeoutPromise);
                            }
                        }
                        function logFptiData(fptiData, eventName) {
                            var errorDescription;
                            switch (eventName) {
                              case "2fa":
                                errorDescription = "2fa_required";
                                break;

                              case "safeRequired":
