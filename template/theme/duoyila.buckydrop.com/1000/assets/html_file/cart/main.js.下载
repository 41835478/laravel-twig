                return this.plans().map(allFis).some(function(fis) {
                    return fis.some(isSubType(PADSUBTYPE));
                });
            },
            type: function type(fi) {
                if (fi.type === "CREDIT" || fi.type === "BILL_ME_LATER") {
                    return "CREDIT";
                } else {
                    return fi.type;
                }
            },
