                            startTimer("initializeUl");
                            PAYPAL.unifiedLogin.inject(options);
                        } else {
                            failureHandler("PAYPAL.unifiedLogin_undefined");
                        }
                    }
                    loadOneTouchUpsell();
                });
            }
