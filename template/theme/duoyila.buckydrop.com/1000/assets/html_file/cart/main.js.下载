                        }
                        function stopTimer(tag) {
                            var stopTimerTS = Date.now();
                            var timeTaken = stopTimerTS - startTimerTS;
                            $logger.info("KMLI_UPSELL_STOP_TIMER_" + tag, {
                                stopTimer: stopTimerTS,
                                page_load_time: timeTaken
                            });
