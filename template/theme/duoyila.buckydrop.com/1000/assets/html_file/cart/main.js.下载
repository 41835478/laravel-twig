                                    type: "success",
                                    msgKeys: [ "paymentMethod.paymentMethodUpdated" ]
                                };
                            }
                            $scope.bml.showModal = false;
                        };
                        $scope.bmlClickContinue = function($event) {
                            $event.preventDefault();
                            $scope.bml.showModalBackdrop = false;
                            $scope.bml.showModal = false;
                        };
                    }
                };
